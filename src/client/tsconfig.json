{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ],
    "outDir": "../../bin/client",
    "module": "commonjs",
    "declaration": true,
    "noImplicitAny": true,
    "strictNullChecks": false,
    "removeComments": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "suppressImplicitAnyIndexErrors": true,
    "sourceRoot": ".",
    "inlineSources": true,
    "baseUrl": ".",
    "paths": {
      "App": [
        "app"
      ],
      "App/*": [
        "app/*"
      ]
    },
    "types": [
      "node"
    ],
    "typeRoots": [
      "../../node_modules/@types"
    ]
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}